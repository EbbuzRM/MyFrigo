{
  "expo": {
    "name": "MyFrigo",
    "slug": "myfrigoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myfrigo",
    "icon": "./assets/images/icon.png",
    "backgroundColor": "#ffffff",
    "splash": {
      "image": "./assets/images/splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.myfrigo",
      "backgroundModes": [
        "fetch",
        "processing"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.myfrigo",
      "launchMode": "singleTask",
      
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "index": "./index.html"
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development",
          "devTeam": "YOUR_DEV_TEAM_ID"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-updates",
      "expo-build-properties",
      "expo-font",
      "expo-web-browser",
      "expo-background-task",
      [
        "expo-splash-screen",
        {
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6120f00b-d739-4a6d-886f-e96cf23c12fb"
      },
      "pexelsApiKey": "U0vYDQ9V9rjDggsDZC6PJCTSC2VpAfR1zYn0fyx0HEzr9c7sv9eQzcfs"
    }
  }
}
